                <br/>
        <!-- Launch grading modal button -->
         <div class="row">
          <div class="col-lg-4 col-centered text-center", style="margin-left: 55em;">
            <!-- Form for search bar -->
            <button type="button" class="btn" data-toggle="modal" data-target="#gradingModal">Edit Grading Formula</button>
          </div><!-- /.col-lg-4 -->
        </div><!-- /.row -->
        <!--Grading Modal -->
        <div class="modal fade" id="gradingModal" tabindex="-1" role="dialog" aria-labelledby="gradingModalLabel">
          <div class="modal-dialog" role="document">
            <div class="modal-content">
              <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span>       </button>
                <h4 class="modal-title" id="gradingModalLabel">Edit Formula</h4>
              </div>
              <div id="grading-modal-body" class="modal-body">
                
              </div>
              <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                <button type="button" id="regradeButton" class="btn btn-primary">Regrade Repositories</button>
              </div>
            </div>
          </div>
        </div><!-- End modal -->
      </div>
    </div>

  <div class="container">
      <!-- Review Form -->
      <div class="row">
          <!-- Check to see if we are adding a review -->
